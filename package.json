{
	"name": "portfolio",
	"version": "1.0.0",
	"engines": {
		"node": "22.12.0",
		"npm": "10.9.0"
	},
	"type": "module",
	"description": "",
	"scripts": {
		"start": "chokidar \"src/**/*.{html,css,svg}\" -c \"npm run build\" & live-server docs",
		"build": "node scripts/build.js",
		"build:prod": "npm run cleanup && npm run svg:optimize && npm run build",
		"cleanup": "rimraf docs",
		"svg:optimize": "svgo src/svg/*",
		"format": "npm run svg:optimize && prettier --write \"{,src/partials/**/}*.{md,json,css,js,html,yml}\""
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"chokidar-cli": "^3.0.0",
		"live-server": "^1.2.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"svgo": "^3.3.2"
	}
}
